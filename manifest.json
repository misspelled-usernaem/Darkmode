{
    "manifest_version": 3,
    "name": "roblox status darkmode",
    "version": "1.0.1",
    "description": "too bright",
    "action":{
      "default_title": "roblox darkmode"
    },
    "content_scripts": [
      {
        "matches": ["https://status.roblox.com/*"],
        "js": ["darkmode.js"],
        "all_frames": false,
        "run_at": "document_idle"
      }
    ]
  }